{
    "name": "blaze-engine",
    "version": "1.0.0",
    "description": "The simplest 2D game engine for React. Use any React library inside your games.",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "source": "src/index.ts",
    "sideEffects": false,
    "files": [
        "src",
        "!**/__tests__",
        "!**/__mocks__",
        "!**/.*"
    ],
    "scripts": {
        "typecheck": "tsc --noEmit",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "keywords": [
        "react",
        "game-engine",
        "2d",
        "game",
        "blaze",
        "canvas",
        "sprite",
        "animation",
        "particles",
        "collision",
        "hooks",
        "components"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "jest": "^29.7.0",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {
        "react": ">=18.0.0"
    },
    "peerDependenciesMeta": {
        "react-dom": {
            "optional": true
        },
        "react-native": {
            "optional": true
        }
    }
}